{
  "name": "Shello",
  "version": "0.3.3",
  "manifest_version": 2,
  "description": "Links your tasks in Trello.com to related user stories in Trello, or Redmine.",
  "browser_action": {
    "default_icon": "img/icon128.png",
    "default_popup": "popup.html"
  },
  "icons" : {
    "16" : "img/icon16.png",
    "48" : "img/icon48.png",
    "128" : "img/icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
  "tabs",
  "storage",
  "https://trello.com/1/OAuthGetRequestToken",
  "https://trello.com/1/OAuthAuthorizeToken",
  "https://trello.com/1/OAuthGetAccessToken",
  "https://trello.com/*",
  "https://redmine.rm.com/*"
  ]
}