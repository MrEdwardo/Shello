{
  "name": "Shello",
  "version": "0.3.2",
  "manifest_version": 2,
  "update_url" : "http://dl.dropbox.com/u/30214/Shello/updates.xml",
  "description": "Links your tasks in Trello.com to related user stories in Trello, or Redmine.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
  "tabs",
  "storage",
  "https://trello.com/1/OAuthGetRequestToken",
  "https://trello.com/1/OAuthAuthorizeToken",
  "https://trello.com/1/OAuthGetAccessToken",
  "https://trello.com/*",
  "https://redmine.rm.com/*"
  ]
}